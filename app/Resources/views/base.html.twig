<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/html">
<head>
    <meta charset="UTF-8" />
    <title>{% block title %}Mobile!{% endblock %}</title>
    {% block javascripts %}
        <script type="text/javascript" src="http://code.jquery.com/jquery-1.6.4.min.js"></script>
        <script type="text/javascript" src="http://code.jquery.com/mobile/1.0/jquery.mobile-1.0.min.js"></script>
        <script src="{{ asset('/js/klass.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('/js/code.photoswipe.jquery-3.0.4.min.js') }}" type="text/javascript"></script>
        <script src="{{ asset('/js/custom.js') }}" type="text/javascript"></script>
    {% endblock %}
    {% block stylesheets %}
        <link href="{{ asset('/css/style.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('/css/photoswipe.css') }}" type="text/css" rel="stylesheet" />
        <link href="{{ asset('/css/jquery.mobile-1.0.min.css') }}" type="text/css" rel="stylesheet" />
    {% endblock %}
    <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>
{% block header %}
    <div data-role="page" data-add-back-btn="true">

        <div data-role="header" id="header">
            <span>
                <a href="{{ path('homepage') }}">
                    <img src="{{ asset('images/home.jpg') }}" height="30px" width="30px" style="float:left; padding-left: 1%;padding-top: 0.2%;" >
                </a>
            </span>
            {% block titlepage %}{% endblock %}
            <span>
                <a href="{{ path('fos_user_profile_show') }}">
                    <img src="{{ asset('images/user.png') }}" height="30px" width="30px" style="float:right; padding-right: 1%;padding-top: 0.2%;" >
                </a>
            </span>
        </div><!-- /header -->
        {% block body %}{% endblock %}
        <div data-role="footer" data-theme="a">
            {% render "MobileFrontendBundle:Footer:index" %}
        </div>
        <!-- /Footer -->
    </div><!-- /page -->
{% endblock %}
</body>
</html>


